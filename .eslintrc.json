{
    "extends": ["airbnb", "prettier", "plugin:node/recommended"],
    "plugins": ["prettier"],
    "rules": {
        "prettier/prettier": "error",
        "no-unused-vars": "warn",
        "no-underscore-dangle": "off",
        "no-console": "off",
        "func-names": "off",
        "object-shorthand": "off",
        "no-loop-func": "off"
    }
}